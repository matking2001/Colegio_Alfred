<div class="card shadow rounded">
  <div class="card-body">
    <%= simple_form_for [@user, @report] do |f| %>
      <%= f.input :titulo, class:"rounded"%>
      <%= f.input :contenido, class:"rounded"%>
      <%= f.input :fecha%>
      <%= f.submit "Enviar", class:"btn btn-primary shadow"%>
    <%end%>
  </div>
</div>
 